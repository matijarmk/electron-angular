{
  "name": "sign-it",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel angular:serve electron:serve",
    "package": "rimraf dist release && npm run angular:build-prod && electron-builder build --publish never",
    "build:prod": "rimraf dist && npm run angular:build-prod && electron .",
    "build:dev": "rimraf dist && npm run angular:build-dev && electron .",
    "angular:serve": "ng serve",
    "angular:build-dev": "ng build --base-href=./ --configuration=development",
    "angular:build-prod": "ng build --base-href=./ --configuration=production",
    "angular:test": "ng test",
    "electron:build": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:build && electron . --serve",
    "ng": "ng",
    "electron": "electron",
    "postinstall": "electron-builder install-app-deps"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@electron/remote": "^1.1.0",
    "keytar": "^7.7.0",
    "pdf-signer": "^1.1.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "win-ca": "^3.4.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.0.0",
    "@angular-devkit/build-angular": "~12.0.2",
    "@angular-eslint/builder": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/schematics": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "electron": "^13.0.1",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-loader": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.2.3",
    "wait-on": "^5.3.0"
  }
}
